{
  "auto": {
    "cleanup": {
      "days": 7,
      "played": false,
      "unfinished": true,
      "unplayed": false
    },
    "retries": 3,
    "update": {
      "enabled": true,
      "frequency": 120
    }
  },
  "device_sync": {
    "after_sync": {
      "delete_episodes": true,
      "mark_episodes_played": false,
      "sync_disks": false
