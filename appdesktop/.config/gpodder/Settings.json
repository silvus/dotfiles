    "custom_sync_name": "{episode.sortdate}_{episode.title}",
    "custom_sync_name_enabled": true,
    "delete_played_episodes": false,
    "device_folder": "/data/podcasts",
    "device_type": "filesystem",
    "max_filename_length": 120,
    "one_folder_per_podcast": true,
    "playlists": {
      "create": false,
      "folder": ".playlists",
      "two_way_sync": true,
      "use_absolute_path": true
    },
    "skip_played_episodes": false
  },
  "downloads": {
    "chronological_order": true
  },
  "extensions": {
    "audio_converter": {
      "context_menu": true,
      "use_ogg": false,
      "use_opus": false
